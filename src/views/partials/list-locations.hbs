 <div class="locations-container">
  {{#each locations}}
    <div class="box box-link-hover-shadow location-box">
      <h2 class="title">
        {{title}}
      </h2>
      <div class="buttons">
        <a href="/location/{{_id}}/locationdetails" class="button is-small">
          <span class="icon is-small">
            <i class="fas fa-folder-open"></i>
          </span>
        </a>
        <a href="/dashboard/deletelocation/{{_id}}" class="button is-small">
          <span class="icon is-small">
            <i class="fas fa-trash"></i>
          </span>
        </a>
      </div>
    </div>
  {{/each}}
</div>